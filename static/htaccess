RewriteEngine on
Options -Indexes
DirectoryIndex index.html

ErrorDocument 404 /errors/404.shtml
ErrorDocument 403 /errors/403.shtml
ErrorDocument 500 /errors/500.html

RewriteCond %{HTTP_HOST} ^www\.datahackermd\.com$ [NC]
RewriteRule ^(.*)$ "http\:\/\/datahackermd\.com\/$1" [R=301]

RewriteCond %{HTTP_HOST} ^datahackermd\.com$ 
RewriteRule ^index\.html$ "http\:\/\/datahackermd\.com\/" [R=301,L]
